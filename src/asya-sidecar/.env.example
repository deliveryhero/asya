# Transport configuration
TRANSPORT_TYPE=sqs  # sqs or rabbitmq
ASYA_QUEUE_NAME=my-actor-queue

# Runtime configuration
ASYA_SOCKET_PATH=/tmp/sockets/app.sock
ASYA_RUNTIME_TIMEOUT=5m

# Terminal queues
ASYA_STEP_HAPPY_END=happy-end
ASYA_STEP_ERROR_END=error-end

# SQS Configuration (when TRANSPORT_TYPE=sqs)
QUEUE_BASE_URL=https://sqs.us-east-1.amazonaws.com/123456789012
AWS_REGION=us-east-1
SQS_VISIBILITY_TIMEOUT=300
SQS_WAIT_TIME_SECONDS=20
SQS_MAX_MESSAGES=1

# RabbitMQ Configuration (when TRANSPORT_TYPE=rabbitmq)
ASYA_RABBITMQ_URL=amqp://guest:guest@localhost:5672/
ASYA_RABBITMQ_EXCHANGE=asya
ASYA_RABBITMQ_PREFETCH=1
